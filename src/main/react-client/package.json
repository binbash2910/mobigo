{
  "name": "mobigo-react-client",
  "version": "0.0.1",
  "description": "Generated TypeScript/Axios API client for Mobigo",
  "scripts": {
    "build": "npm run fetch-spec && npm run generate:from-file",
    "fetch-spec": "curl -o api-docs.json http://localhost:8080/v3/api-docs",
    "generate": "openapi-generator-cli generate",
    "generate:from-file": "openapi-generator-cli generate -i ./api-docs.json -g typescript-axios -o ./generated --skip-validate-spec --additional-properties=supportsES6=true,withInterfaces=true,useSingleRequestParameter=true",
    "generate:local": "openapi-generator-cli generate -i http://localhost:8080/v3/api-docs -g typescript-axios -o ./generated --additional-properties=supportsES6=true,withInterfaces=true,useSingleRequestParameter=true"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.3"
  }
}
